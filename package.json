{
  "name": "salim-quote-delivery",
  "displayName": "Salim Quote Delivery",
  "description": "Delivery Salim Quote at desire",
  "publisher": "Leomotors",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:salim-quote-delivery.getSalimQuote"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "salim-quote-delivery.getSalimQuote",
        "title": "Deliver me Salim Quote!"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.58.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.12",
    "axios": "^0.21.1",
    "node-fetch": "^2.6.1"
  }
}
